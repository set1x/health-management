    {
      "name": "Auth",
      "description": "Authentication entry points used by the login and register views.",
      "item": [
        {
          "name": "Register",
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"{{email}}\",\n  \"password\": \"{{password}}\",\n  \"nickname\": \"{{nickname}}\",\n  \"gender\": \"{{gender}}\",\n  \"dateOfBirth\": \"{{dateOfBirth}}\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/register",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "register"]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('HTTP status is 200', function () {",
                  "  pm.response.to.have.status(200);",
                  "});",
                  "const jsonData = pm.response.json();",
                  "pm.test('Register success or already exists', function () {",
                  "  pm.expect([0, 1]).to.include(jsonData.code);",
                  "});",
                  "if (jsonData.code === 1 && jsonData.data) {",
                  "  pm.environment.set('userId', jsonData.data);",
                  "}"
                ]
              }
            }
          ],
          "response": []
        },
        {
          "name": "Login",
          "request": {
            "auth": {
              "type": "noauth"
