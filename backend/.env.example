# MySQL 配置
MYSQL_ROOT_PASSWORD=your_password_here
MYSQL_PORT=3306

# 数据库连接配置
SPRING_DATASOURCE_URL=jdbc:mysql://localhost:3306/health_management_db?useSSL=false&serverTimezone=Asia/Shanghai
SPRING_DATASOURCE_USERNAME=root

# 后端服务配置
SERVER_PORT=8080
SPRING_PROFILES_ACTIVE=prod

# 阿里云 DashScope API Key
SPRING_AI_DASHSCOPE_API_KEY=your_dashscope_api_key_here

# JWT 安全配置
JWT_SIGN_KEY=your_jwt_sign_key_here
JWT_EXPIRE_TIME=43200000

# 文件上传路径（可选，默认为 src/main/resources/static/avatars/）
AVATAR_UPLOAD_DIR=/path/to/avatars/
